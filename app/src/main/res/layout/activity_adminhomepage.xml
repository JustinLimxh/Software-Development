<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="410dp"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="#E9ECC7">


    <TextView
        android:id="@+id/admintitle"
        android:layout_width="362dp"
        android:layout_height="93dp"
        android:layout_marginTop="152dp"
        android:text="Admin Homepage"
        android:textSize="45sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="347dp"
        android:layout_height="247dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="131dp"
        android:background="#6FA56A"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.409"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admintitle"
        app:layout_constraintVertical_bias="1.0">

        <Button
            android:id="@+id/vfeed"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:background="#DCD5D8"
            android:text="View Feedback"
            android:textColor="#000000"
            app:backgroundTint="@null" />

        <Button
            android:id="@+id/pushnoti"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:background="#DCD5D8"
            android:text="Push Notifications"
            android:textColor="#000000"
            app:backgroundTint="@null" />

        <Button
            android:id="@+id/upmenu"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:background="#DCD5D8"
            android:text="Upload Menu"
            android:textColor="#000000"
            app:backgroundTint="@null" />
    </LinearLayout>

    <Button
        android:id="@+id/btnlogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.051" />


</androidx.constraintlayout.widget.ConstraintLayout>
